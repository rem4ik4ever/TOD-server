{
  "compilerOptions": {
    "strictNullChecks": true,
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "rootDir": ".",
    "baseUrl": ".",
    "paths": {
      "*/": ["./*"],
    }
  },
  "include": ["./src/**/*.ts", "./prisma/**/*"]
}
