{
  "name": "service_template",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prepare": "husky install",
    "clean": "rimraf dist/*",
    "lint": "npx eslint --ext .ts . --fix",
    "tsc": "tsc",
    "build": "npm-run-all clean lint tsc",
    "dev:start": "npm-run-all build start",
    "dev": "nodemon --watch src -e ts --exec ts-node -r tsconfig-paths/register --transpile-only ./src/index.ts",
    "resque": "nodemon --watch src -e ts --exec ts-node -r tsconfig-paths/register --transpile-only ./src/resque.ts",
    "start": "node dist/src/index.js",
    "generate": "ts-node --transpile-only src/api/schema",
    "test": "NODE_ENV=test yarn generate && jest --coverage ./src",
    "test:report": "NODE_ENV=test yarn generate && jest --coverage ./src && npx http-server ./coverage/lcov-report"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.19.0",
    "@aws-sdk/types": "^3.18.0",
    "@godaddy/terminus": "^4.9.0",
    "@prisma/client": "^2.25.0",
    "@sentry/node": "^6.15.0",
    "@sentry/tracing": "^6.15.0",
    "aws-sdk": "^2.935.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "faker": "^5.5.3",
    "graphql": "^15.5.0",
    "graphql-middleware": "^6.0.10",
    "graphql-playground-middleware-express": "^1.7.22",
    "graphql-relay": "^0.7.0",
    "graphql-scalars": "^1.10.0",
    "graphql-shield": "^7.5.0",
    "ioredis": "^4.27.6",
    "iron-session": "^6.0.4",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "nexus": "^1.0.0",
    "node-resque": "^9.0.2",
    "nodemailer": "^6.6.2",
    "papaparse": "^5.3.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.14.5",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/ioredis": "^4.26.4",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.2",
    "@types/lodash": "^4.14.168",
    "@types/morgan": "^1.9.3",
    "@types/node": "^14.14.37",
    "@types/nodemailer": "^6.4.2",
    "@types/papaparse": "^5.2.5",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "get-port": "^5.1.1",
    "graphql-request": "^3.4.0",
    "husky": "^6.0.0",
    "jest": "^27.0.4",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "prisma": "^2.25.0",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.4"
  },
  "jest": {
    "preset": "ts-jest",
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "moduleDirectories": [
      "node_modules",
      ".",
      "./src"
    ],
    "testEnvironment": "node",
    "transform": {
      "node_modules/variables/.+\\.(j|t)sx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!variables/.*)"
    ]
  }
}
