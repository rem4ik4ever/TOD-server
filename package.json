    "faker": "^5.5.3",
    "graphql": "^15.5.0",
    "graphql-relay": "^0.7.0",
    "graphql-scalars": "^1.10.0",
    "lodash": "^4.17.21",
    "nexus": "^1.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.14.5",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.37",
    "@types/supertest": "^2.0.11",
